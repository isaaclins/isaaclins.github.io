{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "configMigration": true,
  "schedule": [
    "every 1 hour every weekday"
  ],
  "branchPrefix": "renovate/",
  "packageRules": [
    {
      "automerge": false,
      "matchPackageNames": ["*"]
    }
  ],
  "autoApprove": false,
  "prHourlyLimit": 0,
  "branchConcurrentLimit": 0
}
